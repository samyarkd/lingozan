{
  "name": "lingo-zan",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "db:migration": "edgedb migration create --schema-dir=./src/dbschema/ --password -P 5656 --tls-security=insecure",
    "db:migrate": "edgedb migrate --password -P 5656 --tls-security=insecure --schema-dir=./src/dbschema/",
    "db:push": "bun db:migration && bun db:migrate",
    "generate:edgeql": "bunx @edgedb/generate  edgeql-js --target ts --password -P 5656 --output-dir=./src/dbschema/edgeql-js --tls-security=insecure",
    "generate:interfaces": "bunx @edgedb/generate interfaces --password -P 5656 --tls-security=insecure --file ./src/dbschema/interfaces.ts",
    "gen:all": "bun generate:edgeql && bun generate:interfaces"
  },
  "dependencies": {
    "@edgedb/auth-nextjs": "^0.3.1",
    "@langchain/anthropic": "^0.1.9",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.1",
    "@trpc/next": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "edgedb": "^1.5.6",
    "framer-motion": "^11.0.12",
    "langchain": "^0.1.30",
    "next": "^14.2.3",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-icons": "^5.0.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "use-hooks": "^2.0.0-rc.5",
    "usehooks-ts": "^3.0.1",
    "vaul": "^0.9.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.5.3",
    "@tailwindcss/typography": "^0.5.13",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.34",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.28.0"
  }
}
